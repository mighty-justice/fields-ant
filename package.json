{
  "name": "@mighty-justice/fields-ant",
  "version": "1.8.0",
  "description": "Standard Ant design component interface for Field Sets",
  "main": "dist/index.js",
  "module": "dist/fieldsAnt.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.js'",
    "build": "tsdx build",
    "deploy": "np",
    "docs:build": "build-storybook",
    "docs:deploy": "storybook-to-ghpages",
    "docs:watch": "start-storybook -p 6006",
    "format": "tsdx lint --fix",
    "lint": "tsdx lint",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "prepublishOnly": "tsdx build",
    "preversion": "npm run lint",
    "start": "tsdx watch",
    "test": "tsdx test",
    "version": "npm run build && git add dist && auto-changelog -p && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/fields-ant.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Mighty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/fields-ant/issues"
  },
  "homepage": "https://mighty-justice.github.io/fields-ant/",
  "prettier": {
    "printWidth": 120,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ]
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "antd": "^3.0.0",
    "lodash": "^4.0.0",
    "mobx": "^4.0.0 || ^5.0.0",
    "mobx-react": "^5.0.0",
    "moment": "^2.0.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@mighty-justice/tester": "^2.0.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/faker": "^5.1.6",
    "@types/flat": "^5.0.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "16.9.23",
    "@types/rosie": "^0.0.38",
    "@types/storybook-readme": "^5.0.5",
    "@types/storybook__addon-info": "^5.2.4",
    "antd": "4.16.13",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.3",
    "core-js": "^3.8.3",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "faker": "^4.1.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.1",
    "jest-environment-jsdom": "^24.7.1",
    "mobx": "^5.0.0",
    "mobx-react": "^5.0.0",
    "np": "^4.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "regenerator-runtime": "^0.13.7",
    "rosie": "^2.0.1",
    "source-map-explorer": "^2.5.0",
    "storybook-readme": "^5.0.9",
    "tsdx": "^0.14.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@mighty-justice/smart-bool": "^2.0.2",
    "@mighty-justice/utils": "1.4.2",
    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "flat": "^5.0.2",
    "http-status-codes": "^1.3.2",
    "iso8601-duration": "^1.2.0",
    "lodash": "^4.0.0",
    "moment": "^2.0.0"
  }
}
