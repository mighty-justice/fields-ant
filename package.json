{
  "name": "@mighty-justice/fields-ant",
  "version": "1.7.1",
  "description": "Standard Ant design component interface for Field Sets",
  "main": "dist/fieldsAnt.cjs.js",
  "module": "dist/fieldsAnt.esm.js",
  "browser": "dist/fieldsAnt.umd.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "analyze": "source-map-explorer --html dist/index.js dist/index.map > source.html && open source.html",
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -r dist/* || true",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "deploy": "np",
    "docs:build": "build-storybook",
    "docs:deploy": "storybook-to-ghpages",
    "docs:watch": "start-storybook -p 6006",
    "format": "tsdx lint --fix",
    "lint": "tsdx lint",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "preversion": "npm run lint",
    "start": "rollup -c -w",
    "test": "jest",
    "test:watch": "npm run test/*.js -- -w",
    "version": "npm run build && git add dist && auto-changelog -p && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/fields-ant.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Mighty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/fields-ant/issues"
  },
  "homepage": "https://mighty-justice.github.io/fields-ant/",
  "prettier": {
    "printWidth": 120,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ],
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "!src/index.ts",
      "!src/props.ts",
      "src/**/*.tsx",
      "src/**/*.ts"
    ]
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "antd": "^3.0.0",
    "lodash": "^4.0.0",
    "mobx": "^4.0.0 || ^5.0.0",
    "mobx-react": "^5.0.0",
    "moment": "^2.0.0",
    "react": "^16.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@mighty-justice/tester": "^2.0.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/faker": "^5.1.6",
    "@types/flat": "^5.0.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "16.9.23",
    "@types/rosie": "^0.0.38",
    "@types/storybook-readme": "^5.0.5",
    "@types/storybook__addon-info": "^5.2.4",
    "antd": "4.16.13",
    "auto-changelog": "^2.2.1",
    "babel-loader": "^8.0.5",
    "babelrc-rollup": "^3.0.0",
    "core-js": "^3.8.3",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "faker": "^4.1.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.1",
    "jest-environment-jsdom": "^24.7.1",
    "mobx": "^5.0.0",
    "mobx-react": "^5.0.0",
    "np": "^4.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rosie": "^2.0.1",
    "source-map-explorer": "^2.5.2",
    "storybook-readme": "^5.0.9",
    "style-loader": "^0.23.1",
    "tsdx": "^0.14.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@mighty-justice/smart-bool": "^2.0.2",
    "@mighty-justice/utils": "1.4.2",
    "@types/cheerio": "^0.22.23",
    "@types/react-dom": "^17.0.11",
    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "flat": "^5.0.2",
    "http-status-codes": "^1.3.2",
    "iso8601-duration": "^1.2.0",
    "lodash": "^4.0.0",
    "moment": "^2.0.0"
  }
}
