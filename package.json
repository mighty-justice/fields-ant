{
  "name": "@mighty-justice/fields-ant",
  "version": "1.3.3-3",
  "description": "Standard Ant design component interface for Field Sets",
  "main": "dist/fieldsAnt.cjs.js",
  "module": "dist/fieldsAnt.esm.js",
  "browser": "dist/fieldsAnt.umd.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -r dist/* || true",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "deploy": "np --any-branch",
    "dev": "rollup -c -w",
    "docs:build": "build-storybook",
    "docs:deploy": "storybook-to-ghpages",
    "docs:watch": "start-storybook -p 6006",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "preversion": "npm run lint",
    "test": "jest",
    "test:watch": "npm run test/*.js -- -w",
    "version": "npm run build && git add dist && auto-changelog -p && git add CHANGELOG.md",
    "view-source-map": "source-map-explorer --html dist/index.js dist/index.map > source.html && open source.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/fields-ant.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Mighty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/fields-ant/issues"
  },
  "homepage": "https://mighty-justice.github.io/fields-ant/",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ],
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "!src/index.ts",
      "!src/props.ts",
      "src/**/*.tsx",
      "src/**/*.ts"
    ]
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "antd": "^3.0.0",
    "lodash": "^4.0.0",
    "mobx": "^4.0.0 || ^5.0.0",
    "mobx-react": "^5.0.0",
    "moment": "^2.0.0",
    "react": "^16.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@mighty-justice/tester": "^1.3.0",
    "@mighty-justice/tslint-config": "^1.0.2",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.3.13",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/faker": "^4.1.9",
    "@types/flat": "^0.0.28",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/react": "16.9.20",
    "@types/rosie": "^0.0.37",
    "@types/storybook-readme": "^5.0.4",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "antd": "^3.26.11",
    "auto-changelog": "^1.16.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babelrc-rollup": "^3.0.0",
    "coveralls": "^3.0.9",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "faker": "^4.1.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "jest-environment-jsdom": "^25.1.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "np": "^5.0.0",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rosie": "^2.0.1",
    "source-map-explorer": "^2.3.0",
    "storybook-readme": "^5.0.1",
    "style-loader": "^1.1.3",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@mighty-justice/smart-bool": "^1.0.0",
    "@mighty-justice/utils": "1.3.6",
    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.9.0",
    "flat": "^5.0.0",
    "http-status-codes": "^1.4.0",
    "iso8601-duration": "^1.2.0",
    "lodash": "^4.0.0",
    "moment": "^2.0.0"
  }
}
