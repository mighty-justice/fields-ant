{
  "name": "@mighty-justice/fields-ant",
  "version": "0.0.24",
  "description": "Eliminate your single-line boolean setting methods",
  "main": "dist/fieldsAnt.cjs.js",
  "module": "dist/fieldsAnt.esm.js",
  "browser": "dist/fieldsAnt.umd.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -r dist/* || true",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "deploy": "np",
    "dev": "rollup -c -w",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:deploy": "gh-pages -d _book",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "lint": "tslint --project .",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "preversion": "npm run lint",
    "test": "jest",
    "test:watch": "npm run test/*.js -- -w",
    "version": "npm run build && git add dist && auto-changelog -p && git add CHANGELOG.md",
    "view-source-map": "source-map-explorer --html dist/index.js dist/index.map > source.html && open source.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mighty-justice/fields-ant.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mobx",
    "utils"
  ],
  "author": "Mighty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mighty-justice/fields-ant/issues"
  },
  "homepage": "https://mighty-justice.github.io/fields-ant/",
  "jest": {
    "testPathIgnorePatterns": [
      "/_book/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "!src/index.ts",
      "src/**/*.tsx",
      "src/**/*.ts"
    ]
  },
  "files": [
    "dist",
    "index.js"
  ],
  "peerDependencies": {
    "@mighty-justice/smart-bool": "0",
    "@mighty-justice/utils": "0",
    "antd": "3",
    "class-autobind-decorator": "3",
    "classnames": "2",
    "date-fns": "1",
    "flat": "4",
    "iso8601-duration": "1",
    "lodash": "4",
    "mobx": "5",
    "mobx-react": "5",
    "moment": "2",
    "react": "16"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@mighty-justice/smart-bool": "^0.3.2",
    "@mighty-justice/tester": "^1.0.4",
    "@mighty-justice/tslint-config": "^0.3.0",
    "@storybook/addon-actions": "^4.1.6",
    "@storybook/addon-info": "^4.1.6",
    "@storybook/addon-links": "^4.1.6",
    "@storybook/addons": "^4.1.6",
    "@storybook/react": "^4.1.6",
    "@types/classnames": "^2.2.7",
    "@types/flat": "^0.0.28",
    "@types/lodash": "^4.14.119",
    "@types/react": "16.7.18",
    "auto-changelog": "^1.10.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babelrc-rollup": "^3.0.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "faker": "^4.1.0",
    "gh-pages": "^2.0.1",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-anchorjs": "^2.1.0",
    "gitbook-plugin-search-plus": "^1.0.4-alpha-3",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "np": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "source-map-explorer": "^1.6.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@mighty-justice/smart-bool": "^0.3.2",
    "@mighty-justice/utils": "^0.0.4",
    "antd": "3.12.1",
    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "flat": "^4.1.0",
    "iso8601-duration": "^1.1.6",
    "lodash": "4.17.11",
    "mobx": "5.8.0",
    "mobx-react": "5.4.3",
    "moment": "^2.23.0",
    "react": "16.7.0"
  }
}
